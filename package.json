{
  "name": "unexpected-build-canary",
  "version": "0.0.0",
  "description": "Automated build tester for Unexpected plugin ecosystem",
  "scripts": {
    "ci-npm": "shoulder -l unexpected | xargs | npm run exec-canary",
    "ci-librariesio": "shoulder -l --librariesio $LIBRARIESIO_API_KEY unexpected | xargs | npm run exec-canary",
    "exec-canary": "fugl check --package unexpected --reporter html --pretest-or-ignore --ci || true",
    "lint": "eslint ./scripts/*",
    "test": "./scripts/fetch-plugins && fugl --config .dont-break.json --reporter spec",
    "publish-breakage": "gh-pages -d breakage",
    "update-plugins": "./scripts/fetch-plugins"
  },
  "author": "Alex J Burke <alex@alexjeffburke.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/alexjeffburke/unexpected-build-canary.git"
  },
  "license": "MIT",
  "dependencies": {
    "fugl": "^1.3.0",
    "ncp": "^2.0.0",
    "semver": "^5.6.0",
    "shoulder": "^0.3.0",
    "unexpected": "^10.40.2"
  },
  "devDependencies": {
    "eslint": "^5.0.0",
    "eslint-config-pretty-standard": "^2.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "gh-pages": "^2.0.1",
    "got": "^8.3.2",
    "prettier": "~1.15.3"
  }
}
